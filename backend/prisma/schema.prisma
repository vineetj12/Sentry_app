generator client {
  provider = "prisma-client-js"
  output   = "../dist/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id            String     @id @default(uuid())
  name          String
  age           Int
  emailAddress  String     @unique
  phoneNumber   String
  nationality   String
  adhaarNumber  String?
  contactName   String
  contactemail  String
  relationship  String
  password      String
  destination   String?
  createdAt     DateTime   @default(now())
  updatedAt     DateTime   @updatedAt
}